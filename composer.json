{
  "name": "carkDigital/events",
  "type": "statamic-addon",
  "license": "proprietary",
  "description": "Statamic addon to create & manage events",
  "authors": [
    {
      "name": "Marc Philipps",
      "email": "hi@carkdigital.de",
      "homepage": "https://carkdigital.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "CarkDigital\\Events\\": "src"
    },
    "files": []
  },
  "autoload-dev": {
    "psr-4": {
      "CarkDigital\\Events\\Tests\\": "tests"
    }
  },
  "extra": {
    "statamic": {
      "name": "Events",
      "description": "Statamic addon to create & manage events"
    },
    "laravel": {
      "providers": [
        "CarkDigital\\Events\\ServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "laravel/framework": "^7.30.3 || ^8.40",
    "spatie/calendar-links": "1.*",
    "statamic/cms": "^3.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^4.1 || ^5.0",
    "phpunit/phpunit": "^8.0 || ^9.0",
    "orchestra/testbench": "^5.0 || ^6.0",
    "spatie/test-time": "^1.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "repositories": []
}
